# MaizeMine benchmark defaults discovered during production benchmark runs.
# Usage:
#   source config/benchmark-maizemine.env
#   /tmp/im-bench-314/bin/python scripts/benchmarks.py --json-out /tmp/maizemine_full_benchmark.json

export INTERMINE314_BENCHMARK_TARGET=maizemine
export INTERMINE314_BENCHMARK_MINE_URL=https://maizemine.rnet.missouri.edu/maizemine/service
export INTERMINE314_BENCHMARK_WORKERS=auto
export INTERMINE314_BENCHMARK_PROFILE=auto
export INTERMINE314_BENCHMARK_REPETITIONS=3
export INTERMINE314_BENCHMARK_MATRIX_SMALL_ROWS=SMALL_MATRIX_ROWS
export INTERMINE314_BENCHMARK_MATRIX_LARGE_ROWS=LARGE_MATRIX_ROWS
export INTERMINE314_BENCHMARK_MATRIX_SMALL_PROFILE=benchmark_profile_4
export INTERMINE314_BENCHMARK_MATRIX_LARGE_PROFILE=benchmark_profile_2
export INTERMINE314_BENCHMARK_MATRIX_STORAGE_COMPARE=1
export INTERMINE314_BENCHMARK_MATRIX_LOAD_REPETITIONS=3

# MaizeMine-safe query shape (avoids Transcript.CDS path incompatibility in legacy intermine).
export INTERMINE314_BENCHMARK_QUERY_ROOT=Gene
export INTERMINE314_BENCHMARK_QUERY_VIEWS=Gene.primaryIdentifier,Gene.secondaryIdentifier,Gene.symbol,Gene.name,Gene.briefDescription,Gene.organism.name,Gene.organism.taxonId
export INTERMINE314_BENCHMARK_QUERY_JOINS=Gene.organism

# Keep "full benchmark" focused on fetch/storage/dataframe matrix; disable targeted exports.
export INTERMINE314_BENCHMARK_TARGETED_EXPORTS=0
